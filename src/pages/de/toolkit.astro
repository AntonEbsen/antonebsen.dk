---
import ToolkitPage from '@components/templates/ToolkitPage.astro';
import { getEntry } from 'astro:content';

export const prerender = true;

const entry = await getEntry('pages', 'de/toolkit');
if (!entry) throw new Error('Could not find content for de/toolkit');
---

<ToolkitPage lang="de" content={entry.data} />
