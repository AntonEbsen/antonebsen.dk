---
import BibliographyPage from '@components/templates/BibliographyPage.astro';
import { getEntry } from 'astro:content';

export const prerender = true;

const entry = await getEntry('pages', 'de/bibliography');
if (!entry) throw new Error('Could not find content for de/bibliography');
---

<BibliographyPage lang="de" content={entry.data} />
