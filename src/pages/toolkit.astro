---
import ToolkitPage from '@components/templates/ToolkitPage.astro';
import { getEntry } from 'astro:content';

export const prerender = true;

const entry = await getEntry('pages', 'da/toolkit');
if (!entry) throw new Error('Could not find content for da/toolkit');
---

<ToolkitPage lang="da" content={entry.data} />
