---
import BibliographyPage from '@components/templates/BibliographyPage.astro';
import { getEntry } from 'astro:content';

export const prerender = true;

const entry = await getEntry('pages', 'da/bibliography');
if (!entry) throw new Error('Could not find content for da/bibliography');
---

<BibliographyPage lang="da" content={entry.data} />
