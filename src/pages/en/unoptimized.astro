---
import UnOptimizedPage from '@components/templates/UnOptimizedPage.astro';
import { getEntry } from 'astro:content';

export const prerender = true;

const entry = await getEntry('pages', 'en/unoptimized');
if (!entry) throw new Error('Could not find content for en/unoptimized');
---

<UnOptimizedPage lang="en" content={entry.data} />
