---
import SpeakingPage from '@components/templates/SpeakingPage.astro';
import { getEntry } from 'astro:content';

export const prerender = true;

const entry = await getEntry('pages', 'en/speaking');
if (!entry) throw new Error('Could not find content for en/speaking');
---

<SpeakingPage lang="en" content={entry.data} />
